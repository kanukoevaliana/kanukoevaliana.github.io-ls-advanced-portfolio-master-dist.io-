.about__left{
    .about__photo{
        width: 200px;
        margin: 0 auto;
    }
    .info{
        margin-top: 80px;
        display: flex;
        .right{
            margin-left: 80px;
            color: #fff;
            &__inf{
                white-space: nowrap;
            }
        }
